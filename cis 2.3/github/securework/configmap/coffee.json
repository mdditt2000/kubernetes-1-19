{
    "class": "AS3",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.13.0",
        "id": "urn:uuid:33045210-3ab8-4636-9b2a-c98d22ab915d",
        "label": "http",
        "remark": "Coffee Template",
        "coffee": {
            "class": "Tenant",
            "coffee_app": {
                "class": "Application",
                "template": "generic",
                "coffee_80_vs": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "172.16.3.4"
                    ],
                    "policyEndpoint": "coffee_cafe.example.com_policy"
                },
                "coffee_cafe.example.com_policy": {
                    "class": "Endpoint_Policy",
                    "rules": [
                        {
                            "name": "coffee_f5_demo_coffee_80",
                            "conditions": [
                                {
                                    "type": "httpHeader",
                                    "name": "host",
                                    "event": "request",
                                    "all": {
                                        "values": [
                                            "cafe.example.com"
                                        ],
                                        "operand": "equals"
                                    }
                                },
                                {
                                    "type": "httpUri",
                                    "name": "1",
                                    "event": "request",
                                    "index": 1,
                                    "pathSegment": {
                                        "values": [
                                            "coffee"
                                        ],
                                        "operand": "equals"
                                    }
                                }
                            ],
                            "actions": [
                                {
                                    "type": "forward",
                                    "event": "request",
                                    "select": {
                                        "pool": {
                                            "use": "coffee_pool"
                                        }
                                    }
                                }
                            ]
                        }
                    ],
                    "strategy": "first-match"
                },
                "coffee_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": []
                        }
                    ]
                }
            }
        }
    }
}